{
  "name": "vue-testing-library",
  "version": "0.4.1",
  "description": "Simple and complete Vue DOM testing utilities that encourage good testing practices.",
  "main": "npm/index.js",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "version": "babel --presets=es2015 src --out-dir npm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfcook/vue-testing-library"
  },
  "keywords": [
    "vue.js",
    "vue.js testing",
    "vue",
    "vue testing",
    "vue.js 2",
    "vue.js 2 testing",
    "vue 2",
    "vue 2 testing"
  ],
  "author": "Daniel Cook",
  "license": "MIT",
  "dependencies": {
    "dom-testing-library": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.12",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "jest": "^22.4.2",
    "jest-in-case": "^1.0.2",
    "jest-serializer-vue": "^1.0.0",
    "vee-validate": "^2.0.6",
    "vue-router": "^3.0.1",
    "vue-jest": "^2.2.1",
    "vuex": "^3.0.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/npm/",
      "<rootDir>/tests/__tests__/components/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
